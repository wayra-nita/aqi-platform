<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name=”viewport” content=”width=device-width, initial-scale=1.0″>
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>{% block title %}Wayra-nita{% endblock %}</title>
        {% block stylesheets %}

{#        <link rel="stylesheet" href="{{ asset('bundles/airwebapp/css/bootstrap/bootstrap.css') }}">#}
        <link rel="stylesheet" href="{{ asset('bundles/airwebapp/css/bootstrap/bootstrap-w.css') }}">

            {# Bootstrap fonts #}
            {% stylesheets output='css/bootstrap/glyphicons-halflings-regular.ttf'
                           '@AirWebAppBundle/Resources/public/css/bootstrap/fonts/glyphicons-halflings-regular.ttf'
            %}{% endstylesheets %}
            {% stylesheets output='css/bootstrap/glyphicons-halflings-regular.eot'
                           '@AirWebAppBundle/Resources/public/css/bootstrap/fonts/glyphicons-halflings-regular.eot'
            %}{% endstylesheets %}
            {% stylesheets output='css/bootstrap/glyphicons-halflings-regular.svg'
                           '@AirWebAppBundle/Resources/public/css/bootstrap/fonts/glyphicons-halflings-regular.svg'
            %}{% endstylesheets %}
            {% stylesheets output='css/bootstrap/glyphicons-halflings-regular.woff'
                           '@AirWebAppBundle/Resources/public/css/bootstrap/fonts/glyphicons-halflings-regular.woff'
            %}{% endstylesheets %}
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}    
            <script type="text/javascript"
                src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDk9eKiJYjvaoDa2g--L4aJvGmqNDrwa8o&sensor=true">
            </script>
            {% javascripts 
                   '@AirWebAppBundle/Resources/public/js/jquery/*'
                   '@AirWebAppBundle/Resources/public/js/bootstrap/*'                   
                   '@AirWebAppBundle/Resources/public/js/backbonejs/underscore1.6.0.min.js'                   
                   '@AirWebAppBundle/Resources/public/js/backbonejs/backbone.min.js'                   
                   '@AirWebAppBundle/Resources/public/js/application/models/*'                   
                   '@AirWebAppBundle/Resources/public/js/application/views/*'                   
                   '@AirWebAppBundle/Resources/public/js/application/main_app.js'                                     
            %}
                <script type="text/javascript" src="{{ asset_url }}"></script>
            {% endjavascripts %}
            {% javascripts '@AirWebAppBundle/Resources/public/js/backbonejs/underscore-min.map' output='js/underscore-min.map' %}
                <script src="{{ asset_url }}"></script>
            {% endjavascripts %}                
            {% javascripts '@AirWebAppBundle/Resources/public/js/backbonejs/backbone-min.map' output='js/backbone-min.map' %}
                <script src="{{ asset_url }}"></script>
            {% endjavascripts %}      
            <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
            <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
        {% endblock %}
    </body>
</html>
